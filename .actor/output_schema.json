{
    "actorOutputSchemaVersion": 1,
    "title": "Instagram Post Scraper Output",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "id": {
            "title": "Post ID",
            "type": "string",
            "description": "Unique Instagram post ID",
            "template": "{{id}}"
        },
        "type": {
            "title": "Post Type",
            "type": "string",
            "description": "Type of Instagram post",
            "template": "{{type}}"
        },
        "shortCode": {
            "title": "Short Code",
            "type": "string",
            "description": "Instagram post shortcode",
            "template": "{{shortCode}}"
        },
        "url": {
            "title": "Post URL",
            "type": "string",
            "description": "Direct URL to the Instagram post",
            "template": "{{url}}"
        },
        "timestamp": {
            "title": "Timestamp",
            "type": "string",
            "description": "ISO 8601 timestamp when the post was created",
            "template": "{{timestamp}}"
        },
        "caption": {
            "title": "Caption",
            "type": "string",
            "description": "Post caption text",
            "template": "{{caption}}"
        },
        "likesCount": {
            "title": "Likes Count",
            "type": "string",
            "description": "Number of likes on the post",
            "template": "{{likesCount}}"
        },
        "commentsCount": {
            "title": "Comments Count",
            "type": "string",
            "description": "Number of comments on the post",
            "template": "{{commentsCount}}"
        },
        "displayUrl": {
            "title": "Display URL",
            "type": "string",
            "description": "URL of the main display image",
            "template": "{{displayUrl}}"
        },
        "username": {
            "title": "Username",
            "type": "string",
            "description": "Username of the post owner",
            "template": "{{username}}"
        },
        "inputUrl": {
            "title": "Input URL",
            "type": "string",
            "description": "Original profile URL that was scraped",
            "template": "{{inputUrl}}"
        }
    }
}
